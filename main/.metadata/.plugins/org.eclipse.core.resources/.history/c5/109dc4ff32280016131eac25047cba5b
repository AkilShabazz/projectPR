import numpy as np

class Voigt_ForceLength(object):
    '''
    classdocs
    '''


    def __init__(self, b, k, force, timeStep):
        '''
        Constructor
        '''
        
        self.b = b
        self.k = k
        self.force = force
        self.timeStep = timeStep
        
        
    def computeLength(self, t):
        timeIndex = np.round(t / self.timeStep)
        
        
        
    def dLdt(self, t):
        timeIndex = np.round(t / self.timeStep)